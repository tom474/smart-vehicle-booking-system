{
	"name": "server",
	"version": "1.0.0",
	"private": true,
	"description": "",
	"main": "src/app.ts",
	"scripts": {
		"start": "node dist/app.js",
		"dev": "nodemon src/app.ts",
		"build": "tsc",
		"clean": "rimraf dist",
		"seed": "npm run seed:master && npm run seed:mock",
		"seed:master": "ts-node seeds/master-data/seed-master.ts",
		"seed:master:update": "ts-node seeds/master-data/update-master.ts",
		"seed:mock": "ts-node seeds/mock-data/seed-mock.ts",
		"seed:production": "node dist/seeds/master-data/seed-master.js && node dist/seeds/mock-data/seed-mock.js",
		"format": "prettier --write \"**/*.{ts,js,json,md}\"",
		"format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
		"lint": "eslint \"**/*.{ts,js,json,md}\"",
		"lint:fix": "eslint \"**/*.{ts,js,json,md}\" --fix",
		"typeorm": "npm run build && npx typeorm -d dist/config/database.js",
		"migration:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -d src/config/database.ts",
		"migration:revert": "npm run typeorm -- migration:revert",
		"migration:generate": "npx ts-node-esm ./node_modules/typeorm/cli.js migration:generate src/database/migrations/auto-migration -d src/config/database.ts"
	},
	"keywords": [],
	"author": "WAO Team",
	"license": "ISC",
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@types/archiver": "^6.0.3",
		"@types/bcrypt": "^6.0.0",
		"@types/body-parser": "^1.19.6",
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/multer": "^2.0.0",
		"@types/node": "^24.1.0",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.3",
		"globals": "^16.3.0",
		"nodemon": "^3.1.10",
		"pino-pretty": "^13.0.0",
		"prettier": "^3.6.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0"
	},
	"dependencies": {
		"@azure/storage-blob": "^12.28.0",
		"@fast-csv/format": "^5.0.5",
		"@googlemaps/google-maps-services-js": "^3.4.2",
		"archiver": "^7.0.1",
		"axios": "^1.11.0",
		"bcrypt": "^6.0.0",
		"body-parser": "^2.2.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.2",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"cronstrue": "^3.2.0",
		"dotenv": "^17.2.1",
		"es6-shim": "^0.35.8",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"multer": "^2.0.2",
		"node-cron": "^4.2.1",
		"pg": "^8.16.3",
		"pg-query-stream": "^4.10.3",
		"pino": "^9.7.0",
		"pino-http": "^10.5.0",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^6.0.1",
		"routing-controllers": "^0.11.2",
		"typedi": "^0.10.0",
		"typeorm": "^0.3.25"
	}
}
